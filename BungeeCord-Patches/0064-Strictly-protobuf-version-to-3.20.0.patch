Date: Thu, 14 Apr 2022 14:56:38 -0300
Subject: [PATCH] Strictly protobuf version to 3.20.0


diff --git a/api/pom.xml b/api/pom.xml
index 379b6023..57c0492c 100644
--- a/api/pom.xml
+++ b/api/pom.xml
@@ -15,6 +15,10 @@
     <version>1.18-R0.1-SNAPSHOT</version>
     <packaging>jar</packaging>
 
+    <properties>
+        <protobuf.version>3.20.0</protobuf.version>
+    </properties>
+
     <name>Waterfall-API</name>
     <description>API implemented by the Elastic Portal Suite</description>
 
@@ -76,6 +80,14 @@
             <version>1.30</version>
             <scope>compile</scope>
         </dependency>
+        <!--Waterfall - Strictly protobuf version-->
+        <!-- https://mvnrepository.com/artifact/com.google.protobuf/protobuf-java -->
+        <dependency>
+            <groupId>com.google.protobuf</groupId>
+            <artifactId>protobuf-java</artifactId>
+            <version>${protobuf.version}</version>
+        </dependency>
+
         <!-- Waterfall - Add SLF4J -->
         <dependency>
             <groupId>org.slf4j</groupId>
